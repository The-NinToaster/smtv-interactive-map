{
  "name": "smtv-interactive-map",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://www.thenintoaster.com/smtv-interactive-map",
  "scripts": {
    "start": "node ./bin/www",
    "startdev": "npm run build & nodemon ./bin/www",
    "build": "npm install & browserify map.js -o public/javascripts/map.js & cp node_modules/leaflet/dist/leaflet.css public/stylesheets/leaflet.css",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d ."
  },
  "dependencies": {
    "cookie": "^0.5.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "^4.18.2",
    "gh-pages": "^5.0.0",
    "leaflet": "^1.9.4",
    "leaflet-rastercoords": "^1.0.5",
    "morgan": "~1.9.1",
    "parser": "^0.1.4"
  },
  "devDependencies": {
    "browserify": "^17.0.0",
    "nodemon": "^2.0.22"
  }
}
